<h1>新規投稿</h1>

  <%= form_for(@post, url: posts_path) do |f| %>

    <div>
      <%= f.label :title %><br>
      <%= f.text_field :title %>
    </div>

    <div>
      <%= f.label :content %><br>
      <%= f.text_area :content %>
    </div>

    <div>
      <%= f.submit "投稿" %>
    </div>
  <% end %>